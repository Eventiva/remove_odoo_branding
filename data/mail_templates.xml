        <template id="auth_signup_reset_password_no_branding"
            inherit_id="auth_signup.reset_password_email"
            name="Remove Odoo Branding from Reset Password Email"
            active="False"
            customize_show="True"
            optional="enabled">
            <xpath expr="//td[p[contains(., 'Powered by')]]" position="replace">
                <!-- Powered by Odoo footer removed -->
            </xpath>
        </template>

